# Firebase Configuration
# Copy this file to .env and fill in your Firebase project details

VITE_API_KEY=your_firebase_api_key_here
VITE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_PROJECT_ID=your_firebase_project_id
VITE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_APP_ID=your_firebase_app_id

# Note: These are public environment variables (prefixed with VITE_)
# They will be included in the client-side bundle
# For sensitive data, use server-side environment variables
